{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ESNext", "DOM.Iterable"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false
  },
  "include": ["src/**/*", "src/components/contact/. contact.box.tsx"],
  "exclude": ["./src/typings/**/*", "node_modules", "./src/cdn/**/*","dist"]
}
