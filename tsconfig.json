{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["DOM", "ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "allowUnreachableCode": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true
    },
    "paths": {
        "@components/*": ["src/components/*"],
        "@engine/*": ["src/_engine/*"],
        "@types/*": ["src/typings/*"]
    },
    "include": ["src/**/*", "src/components/contact/. contact.box.tsx"],
    "exclude": ["src/typings/**/*"]
}
